@model FMS2.Models.ManageViewModels.LogsListViewModel
@{ var content = "";
    if (Model != null && Model.Lines != null) {
        foreach (var item in Model.Lines)
        {
                content += item + "\n";
        }   
    }
}
<textarea class="log-area" wrap="hard" id="log-area" readonly>
    @content
</textarea>
<script>
    scrollLogAreaToEnd();
</script>