<div id="downloadPartial" class="form-group">
    <div id="progress-bar" class="progress" hidden>
        <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%"></div>
    </div>
    <form asp-action="CancelDownloadAsync" asp-controller="File" method="post">
        <p id="info">You have about 10s to cancel archiving process.</p>
        <button id="cancelArchivingButton" type="submit" class="btn btn-primary" data-dismiss="modal">Cancel</button>
    </form>
</div>