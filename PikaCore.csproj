<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <PackageVersion>2.2.2</PackageVersion>
        <Authors>Arkasian</Authors>
        <Description>Pika Core Cloud</Description>
        <PackageProjectUrl>https://core.lukas-bownik.net</PackageProjectUrl>
        <RepositoryUrl>https://www.github.com/Arkasian/pika-core</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>cms core cloud</PackageTags>
        <AssemblyVersion>2.2.2</AssemblyVersion>
        <FileVersion>2.2.2</FileVersion>
        <Version>2.2.2</Version>
    </PropertyGroup>

    <ItemGroup>
